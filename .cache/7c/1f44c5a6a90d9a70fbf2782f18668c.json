round: $primary-bg;\r\n      }\r\n\r\n      .hamburger::after {\r\n        opacity: 0;\r\n      }\r\n\r\n      .hamburger::before {\r\n        transform: rotate(45deg);\r\n      }\r\n\r\n      .hamburger {\r\n        transform: rotate(-45deg);\r\n      }\r\n    }\r\n\r\n    .hamburger {\r\n      height: 2px;\r\n      width: 30px;\r\n      background: $primary-fontColor;\r\n      transform: rotate(0deg);\r\n      transition: transform 0.3s;\r\n\r\n      &::before {\r\n        content: \"\";\r\n        position: absolute;\r\n        bottom: 0;\r\n        height: 2px;\r\n        width: 30px;\r\n        background: $primary-fontColor;\r\n        transform: rotate(0deg);\r\n        transition: transform 0.3s;\r\n      }\r\n\r\n      &::after {\r\n        content: \"\";\r\n        position: absolute;\r\n        bottom: 50%;\r\n        height: 2px;\r\n        width: 30px;\r\n        transform: translateY(50%);\r\n        background: $primary-fontColor;\r\n        opacity: 1;\r\n        transition: opacity 0.3s;\r\n      }\r\n    }\r\n  }\r\n}\r\n\r\nimg {\r\n  width: 100%;\r\n}\r\n\r\na {\r\n  text-decoration: none;\r\n}\r\n\r\n.container {\r\n  width: 90%;\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n}\r\n\r\n.hero {\r\n  height: 70vh;\r\n  overflow: hidden;\r\n\r\n  &::after {\r\n    content: \"\";\r\n    position: absolute;\r\n    background: $primary-overlay;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n  }\r\n\r\n  #video-bg {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    min-width: 100%;\r\n    min-height: 100%;\r\n    width: auto;\r\n    height: auto;\r\n  }\r\n  video {\r\n    display: block;\r\n  }\r\n  .video-container {\r\n    min-height: 100%;\r\n    min-width: 100%;\r\n    height: auto;\r\n    width: auto;\r\n    overflow: hidden;\r\n    position: absolute;\r\n    top: 0;\r\n    right: 0;\r\n    z-index: -100;\r\n  }\r\n}\r\n\r\n.social-icons {\r\n  position: absolute;\r\n  bottom: 2%;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  z-index: 1;\r\n\r\n  i {\r\n    color: $primary-fontColor;\r\n    font-size: 2rem;\r\n    margin: 0 0.5em;\r\n  }\r\n}\r\n\r\n/* Media Queries */\r\n\r\n@media all and (max-width: 767px) {\r\n  .nav-list {\r\n    position: fixed;\r\n    top: 0;\r\n    left: 0;\r\n    height: 100%;\r\n    width: 100%;\r\n    opacity: 0;\r\n    visibility: hidden;\r\n    pointer-events: none;\r\n    transform: translateY(-100%);\r\n    background: white;\r\n    transition: all 0.8s;\r\n\r\n    &.open {\r\n      opacity: 1;\r\n      visibility: visible;\r\n      pointer-events: visible;\r\n      transform: translateY(0);\r\n    }\r\n  }\r\n\r\n  .toggle {\r\n    display: block !important;\r\n  }\r\n\r\n  nav {\r\n    justify-content: flex-end !important;\r\n  }\r\n\r\n  .container-header {\r\n    grid-template-columns: 65% 35% !important;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"c715adb3d2641beb2a6e2c084676aea8","cacheData":{"env":{}}}